{
    "type": "class",
    "name": "PostClass",
    "includes": ["post.h"],
    "path": "posts",

    "methods": {
        "listPosts": {
            "verb": "GET",
            "returns": "QList<Post>"
        },
        "post": {
            "verb": "GET",
            "pathParams": ["id,int"],
            "returns": "Post"
        },
        "savePost": {
            "verb": "POST",
            "body": "Post"
        },
        "deleteUserPosts": {
            "verb": "DELETE",
            "parameters": ["userId,int"]
        }
    }
}