{
	"type": "class",
	"name": "PostClass",
	"includes": ["post.h"],
	"path": "posts",

	"methods": {
		"listPosts": {
			"verb": "GET",
			"returns": "QList<Post>"
		},
		"post": {
			"verb": "GET",
			"pathParams": ["id,int"],
			"returns": "Post"
		},
		"savePost": {
			"verb": "POST",
			"body": "Post",
			"returns": "Post"
		},
		"updatePost": {
			"verb": "PUT",
			"pathParams": ["id,int"],
			"body": "Post",
			"returns": "Post"
		},
		"deletePost": {
			"verb": "DELETE",
			"pathParams": ["id,int"],
			"returns": "void"
		}
	}
}
