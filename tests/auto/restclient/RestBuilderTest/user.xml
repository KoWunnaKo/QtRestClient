<?xml version="1.0" encoding="UTF-8"?>
<RestObject name="User"
			base="QObject"
			registerConverters="true"
			testEquality="false"
			generateEquals="true"
			generateReset="true">
	<Enum name="GenderType">
		<Key>Male</Key>
		<Key>Female</Key>
	</Enum>
	<Enum name="UserType" base="int" isFlags="true">
		<Key value="0x00">Person</Key>
		<Key value="0x01">Sir</Key>
		<Key value="0x02">Doctor</Key>
		<Key value="0x04">Professor</Key>
		<Key value="(Person|Sir|Doctor|Professor)">Dictator</Key>
	</Enum>

	<Property key="id" type="int">-1</Property>
	<Property key="name" type="QString" asStr="true">Anonymous</Property>
	<Property key="gender" type="User::GenderType">User::Male</Property>
	<Property key="title" type="User::UserTypes">User::Person</Property>
</RestObject>
