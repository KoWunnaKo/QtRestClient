{
	"type": "api",
	"name": "TestApi",
	"includes": [
		"post.h",
		"user.h",
		"api_posts.h"
	],
	"globalName": "localhost",
	"autoCreate": true,
	"baseUrl": "http://localhost:3000",
	"apiVersion": "1.2.0",
	"parameters": {
		"limit": "100"
	},
	"headers": {
		"Secret": "baum"
	},

	"classes": {
		"posts": "PostClass"
	},

	"methods": {
		"findUser": {
			"url": "http://localhost:3000/user",
			"verb": "GET",
			"parameters": [
				"id,int"
			],
			"headers": {
				"Request-Style": "barbaric"
			},
			"returns": "User*",
			"except": "QObject*"
		}
	}
}
